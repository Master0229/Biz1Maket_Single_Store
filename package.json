{
  "name": "Biz1Pos",
  "version": "0.0.4",
  "main": "main.js",
  "author": "Master",
  "description": "Biz1Pos",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "hmr": "ng serve --configuration=hmr --open",
    "build-demo": "ng build --prod --build-optimizer --configuration=demo",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{ts,scss,less}\"",
    "tslint": "tslint \"src/**/*.{ts}\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "postinstall": "ngcc",
    "electron-r": "ng build --base-href ./",
    "build-electron-win": "electron-packager ./ --platform=win32 --arch=ia32 --overwrite --asar --icon=src/assets/images/256_Biz1pos.ico",
    "assemble": "npm run electron-r && npm run build-electron-win && npm run gen",
    "start:electron": "ng build --base-href ./ && electron .",
    "run:electron": "electron .",
    "gen": "node gen.js",
    "build": "electron-packager . --out=dist --asar --overwrite --all",
    "build-installer": "electron-builder",
    "setup-win": "electron-packager . Biz1Pos --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/images/256_Biz1pos.ico --prune=true --out=release-builds --version-string.CompanyName=BizDom --version-string.FileDescription=POS --version-string.ProductName=\"Biz1Pos\""


  },
  "build": {
    "appId": "www.biz1pos.bizdom.co.in",
    "productName": "Biz1Pos 0.0.4",
    "win": {
      "target": "nsis",
      "icon": "src/assets/images/256_Biz1pos.ico"
    },
    "nsis": {
      "installerIcon": "256_Biz1pos.ico",
      "uninstallerIcon": "256_Biz1pos.ico",
      "uninstallDisplayName": "Biz1Pos Uninstall",
      "license": "src/assets/license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut" : true,
      "createStartMenuShortcut" : true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts}": [
      "tslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
